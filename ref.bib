@article{zhavoronkov2017,
author = {Nickolai Zhavoronkov and Mikhail Ivanov},
journal = {Opt. Lett.},
keywords = {Harmonic generation and mixing ; Upconversion; X-rays, soft x-rays, extreme ultraviolet (EUV); Attosecond pulses; High harmonic generation; Laser beams; Light matter interactions; Phase matching; Phase shift},
number = {22},
pages = {4720--4723},
publisher = {OSA},
title = {Extended ellipticity control for attosecond pulses by high harmonic generation},
volume = {42},
month = {Nov},
year = {2017},
url = {http://ol.osa.org/abstract.cfm?URI=ol-42-22-4720},
doi = {10.1364/OL.42.004720},
abstract = {Attosecond (1\&\#x2009;\&\#x2009;as$=$10\&\#x2212;18\&\#x2009;\&\#x2009;s) pulses produced through high harmonic generation (HHG) are a basis for studies of electron dynamics during light-matter interaction on an electron\&\#x2019;s natural time scale. Extensively exploited HHG technology has, however, a few unsolved problems, where producing of circularly polarized or chiral attosecond pulses belongs to them. We have demonstrated experimentally a way to control the ellipticity of attosecond pulse trains produced via HHG in two-color, bi-circular laser fields. We show that the combination of a nonlinear medium position and the intensities of the two-color driving laser fields create an effective helicity-dependent filter. Based on this approach, we report generation of chiral spectra providing potential to produce attosecond pulses with polarization tuned from the rotating, but linear to highly elliptic, with ellipticity as much as \&\#x03F5;$=$0.75. This new way to create a chiral-sensitive element offers a simple and practical knob to control polarization for a combined harmonics field in a smooth and predictable manner.},
}



@article{Zhou1994,
author = {Jianping Zhou and Greg Taft and Chung-Po Huang and Margaret M. Murnane and Henry C. Kapteyn and Ivan P. Christov},
journal = {Opt. Lett.},
keywords = {Laser coupling; Laser crystals; Mode locking; Numerical simulation; Self phase modulation; Ti:sapphire lasers},
number = {15},
pages = {1149--1151},
publisher = {OSA},
title = {Pulse evolution in a broad-bandwidth Ti:sapphire laser},
volume = {19},
month = {Aug},
year = {1994},
url = {http://ol.osa.org/abstract.cfm?URI=ol-19-15-1149},
doi = {10.1364/OL.19.001149},
abstract = {We demonstrate that by operating near the zero second- and third-order dispersion point in a self-mode-locked Ti:sapphire laser we can generate sub-10-fs pulses. Our numerical simulations show that the pulse duration is limited by fourth-order dispersion and that shorter pulses will be possible if this can be reduced. Also, by inserting a pellicle in various positions in a Ti:sapphire cavity, we have measured the intracavity pulse duration and chirp of the circulating pulse in the laser. Our results demonstrate that the pulse is shortest near the middle of the laser crystal, in one direction of propagation. In the other direction of propagation, the pulse is positively chirped and several times longer.},
}

