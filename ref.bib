@article{huang2018b,
author = {Cheng Huang and Mingmin Zhong and Zhengmao Wu},
journal = {Opt. Express},
keywords = {Electric fields; Electrons; Extreme ultraviolet; Forward scattering; Light propagation; Soft x rays},
number = {20},
pages = {26045--26056},
publisher = {OSA},
title = {Intensity-dependent two-electron emission dynamics in nonsequential double ionization by counter-rotating two-color circularly polarized laser fields},
volume = {26},
month = {Oct},
year = {2018},
url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-26-20-26045},
doi = {10.1364/OE.26.026045},
abstract = {Nonsequential double ionization of helium in counter-rotating two-color circularly polarized laser fields is investigated with a three-dimensional classical ensemble model. At moderate intensity, the momentum distribution of the two electrons shows a maximum in the middle of each side of the triangle of the negative vector potential. At high intensity, the momentum distribution exhibits a double-triangle structure, which is attributed to the different values of the laser intensity where the two electrons are released after recollision. At low intensity, the momentum distribution shows a shift deviating from the middle of the side of the triangle of the negative vector potential. This is because the first electrons are emitted within a narrow time window after the field maximum. In addition, at low intensity, double-recollision events and NSDI originating from doubly excited states induced by recollision are prevalent.},
}

