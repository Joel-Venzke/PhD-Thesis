journal = {Opt. Express},
keywords = {Coherence; Free-electron lasers (FELs); X-ray interferometry; X-rays, soft x-rays, extreme ultraviolet (EUV); Free electron lasers; Laser sources; Multiphoton processes; Soft x rays; Spatial frequency; Spontaneous emission},
number = {24},
